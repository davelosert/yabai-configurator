{
  "name": "yco",
  "version": "1.1.0",
  "description": "A configurator to create a common .yabairc, .skhdrc and ubersicht configuration from one JSON config",
  "author": "David Losert",
  "bin": {
    "yco": "src/bin/yco.js"
  },
  "scripts": {
    "lint": "eslint src/ tests/",
    "test": "riteway -r esm 'src/**/*.test.js' | tap-nirvana",
    "test:ci": "npm run test && npm run test:integration",
    "test:integration": "mocha 'tests/integration/**/*.test.js'",
    "test:watch": "watch 'clear && npm run -s test | tap-nirvana' src/"
  },
  "dependencies": {
    "buntstift": "4.0.17",
    "command-line-interface": "3.6.15",
    "esm": "3.2.25",
    "ramda": "0.27.1"
  },
  "devDependencies": {
    "@semantic-release/git": "9.0.0",
    "@semantic-release/npm": "7.0.9",
    "eslint": "7.16.0",
    "isolated": "3.0.10",
    "mocha": "8.2.1",
    "nyc": "15.1.0",
    "riteway": "6.2.1",
    "semantic-release": "17.3.0",
    "tap-nirvana": "1.1.0",
    "watch": "1.0.2"
  },
  "license": "MIT"
}
