{
  "name": "yabai-configurator",
  "version": "0.0.1",
  "description": "A configurator to create a common .yabairc, .skhdrc and ubersicht configuration from one JSON config",
  "main": "index.js",
  "bin": {
    "yco": "./build/bin/yco.js"
  },
  "scripts": {
    "build": "rm -rf ./build && babel ./src --out-dir build",
    "test": "riteway -r @babel/register 'src/**/*.test.js' | tap-nirvana",
    "watch": "watch 'clear && npm run -s test | tap-nirvana' src/"
  },
  "author": "David Losert",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/plugin-transform-runtime": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-typescript": "7.12.7",
    "@babel/register": "7.12.10",
    "@babel/runtime-corejs2": "7.12.5",
    "eslint": "7.16.0",
    "nyc": "15.1.0",
    "riteway": "6.2.1",
    "tap-nirvana": "1.1.0",
    "watch": "1.0.2"
  },
  "dependencies": {
    "buntstift": "4.0.17",
    "command-line-interface": "3.6.15",
    "ramda": "0.27.1"
  }
}
