{
  "name": "yco",
  "version": "1.4.0",
  "description": "A configurator to create a common .yabairc, .skhdrc and ubersicht configuration from one JSON config",
  "author": "David Losert",
  "bin": {
    "yco": "src/bin/yco.js",
    "yco-switch-focus": "src/bin/yco-switch-focus.js"
  },
  "scripts": {
    "start": "yco --help",
    "lint": "eslint src/ tests/",
    "test": "riteway 'src/**/*.test.js' | tap-nirvana",
    "test:ci": "npm run test && npm run test:integration",
    "test:integration": "mocha 'tests/integration/**/*.test.js'",
    "test:watch": "watch 'clear && npm run -s test | tap-nirvana' src/"
  },
  "dependencies": {
    "ajv": "6.12.6",
    "better-ajv-errors": "0.7.0",
    "buntstift": "4.0.19",
    "command-line-interface": "4.0.0",
    "ramda": "0.27.1"
  },
  "devDependencies": {
    "@semantic-release/git": "9.0.0",
    "@semantic-release/npm": "7.0.10",
    "eslint": "7.18.0",
    "eslint-plugin-json-format": "2.0.1",
    "hamjest": "3.7.1",
    "isolated": "3.0.11",
    "mocha": "8.2.1",
    "nyc": "15.1.0",
    "riteway": "6.2.1",
    "semantic-release": "17.3.7",
    "tap-nirvana": "1.1.0",
    "watch": "1.0.2"
  },
  "license": "MIT"
}
